#include <bits/stdc++.h> 
vector<vector<long long int>> printPascal(int n) 
{
     vector<long long int> a;
        vector<vector<long long int>> ans;
        long long i=0,j=0;
       
        while(n--){
             a.push_back(1);
            for(j=1;j<i;++j){
                a.push_back(ans[i-1][j]+ans[i-1][j-1]);
            }
            if(i)a.push_back(1);
            ans.push_back({});
            swap(ans.back(),a);
            ++i;
        }
        return ans;
  // Write your code here.
}
